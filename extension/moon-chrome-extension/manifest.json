{
  "name": "Moon Web Start",
  "description" : "Automatically open Moon when creating a new tab.",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "tabs"
  ],
  "action": {
    "default_icon": {
      "16": "img/moon_16.png",
      "32": "img/moon_32.png",
      "48": "img/moon_48.png",
      "128": "img/moon_128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}